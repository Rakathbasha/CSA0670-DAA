nclude <stdio.h>
int main() {

int a[100],target,n,i,j,k;
printf("enter the no of elements:");
scanf("%d",&n);
printf("Enter the target:");
scanf("%d",&target);
printf("enter the elements of the array:");
for(k=0;k<n;k++)
	scanf("%d ",&a[k]);

for(i=0;i<n;i++){
	for(j=i+1;j<n;j++){
		if(a[i]+a[j]==target){
			printf("The target is equal to sum of%d %d",a[i],a[j]);
		}
	}
}
}
